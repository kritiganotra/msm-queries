<h1>List of all actors</h1>
<hr>
<table border = "1">
  <tr>
    <th>ID</th>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th></th>
  </tr>

  <% Actor.all.each do |a| %>
  <tr>
    <td><%=a.id%></td>
    <td><img src ="<%=a.image%>" width = "100" </td>
    <td><%=a.name%></td>
    <td><%=a.dob%></td>
    <td><a href = "/actors/<%=a.id%>">Show details</a></td>
  </tr>
  <% end %>
</table>
